[Unit]
Description={{ systemd.service_name }}

[Service]
Environment=TZ=:/etc/localtime
ExecStart={{ this.start_f }}
ExecStop={{ this.stop_f }}
Group={{ this.run_u }}
SyslogIdentifier={{ systemd.service_name }}
TimeoutStartSec=10min
Type=forking
User={{ this.run_u }}

[Install]
WantedBy=multi-user.target
