{
    "hosts": ["unix://", "tcp://10.10.10.60:2376"],
    "data-root": "/srv/docker",
    "iptables": false,
    "live-restore": true,
    "log-driver": "journald",
        "tls": true,
        "tlscacert": "/etc/docker/tls/cacert.pem",
        "tlscert": "/etc/docker/tls/cert.pem",
        "tlskey": "/etc/docker/tls/key.pem",
        "tlsverify": true,
    "storage-driver": "overlay2",
    "storage-opts": [
        "overlay2.override_kernel_check=true"
    ]
}
